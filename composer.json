{
    "name": "thavarshan/xoomp",
    "type": "package",
    "description": "A PHP video thumbnail generator.",
    "keywords": ["video", "thumbnail", "xoomp"],
    "authors": [
        {
            "name": "Thavarshan Thayananthajothy",
            "email": "tjthavarshan@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "php-ffmpeg/php-ffmpeg": "^0.17.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Xoomp\\": "src/Xoomp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xoomp\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
